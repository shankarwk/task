 <h1>Login</h1>



<%= form_with  method: :post, url:login_path  do |form| %>


    <div>
    <%= form.label :email %><br>
     <%= form.email_field :email%>
  </div>

   <div>
    <%= form.label :password %><br>
    <%= form.password_field :password %>
  </div >


  <div>
    <%= form.submit "Login" %>
  </div>
<% end %>